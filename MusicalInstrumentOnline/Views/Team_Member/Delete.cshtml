@model IEnumerable<MusicalInstrumentOnline.Models.TeamMember>
@{
    Layout = "~/Views/Shared/_Admin.cshtml";
}
<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>TeamMember</h4>
    <hr />
    @foreach (var item in Model)
    {
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Id)
        </dt>
        <dd class = "col-sm-10">
            @item.Id
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.name)
        </dt>
        <dd class = "col-sm-10">
            @item.name
        </dd>
        <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.emageFile)
        </dt>
        <dd class = "col-sm-10">
                <img src=@Url.Content("~/Home/img/" + item.emagePath) asp-append-version="true"
                     width="100" height="100" />
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.description)
        </dt>
        <dd class = "col-sm-10">
            @item..description
        </dd>
    </dl>
    }
    <form asp-action="Delete">
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index">Back to List</a>
    </form>
</div>
